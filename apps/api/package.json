{
  "name": "@sass/api",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsx watch src/index.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --no-errors-on-unmatched --write .",
    "lint:staged": "biome check --no-errors-on-unmatched --staged .",
    "lint:ci": "biome ci --no-errors-on-unmatched ."
  },
  "dependencies": {
    "@sass/auth": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@sass/biome-config": "workspace:*",
    "@sass/typescript-config": "workspace:*",
    "tsx": "^4.19.1"
  }
}
